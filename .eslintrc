{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "prettier"],
	"extends": ["plugin:@typescript-eslint/eslint-recommended", "plugin:@typescript-eslint/recommended", "prettier"],
	"rules": {
		"no-useless-catch": "warn",
		"prettier/prettier": [
			"warn",
			{
				"useTabs": true,
				"tabWidth": 4,
				"singleQuote": true,
				"printWidth": 120,
				"trailingComma": "none",
				"semi": false
			}
		]
	}
}
